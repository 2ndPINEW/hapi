    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "next": "13.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "vercel": "^28.12.4",
    "wrangler": "^2.7.1"
  }
